<%= render :partial => 'shared/admin_edit_actions', :locals => {:record => @download} %>
<ul class="actions">
  <li><%= link_to('Rename', rename_admin_download_path(@download)) %></li>
</ul>

<h1><%= calculate_page_heading %></h1>

<%= form_for(:download, :url => admin_download_path(@download), :html => { :multipart => true, :method => :put }) do |f| -%>
  <%= render :partial => "form", :locals => {:f => f} %>
	<%= submit_or_cancel(admin_download_path(@download)) %>
<% end -%>