<%= content_tag_for(:tr, version, :class => cycle('odd', 'even')) do -%>
  <td class="icon"><%= version.number %></td>
  <td><%= version.created_at.to_s(:custom_with_time) %></td>
  <td><%= version.user %></td>
  <td class="icon">
    <%= compare_link(versionable, version) %>
  </td>
  <td class="icon">
    <%= revert_link(versionable, version) %>
  </td>
<% end %>
